{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "allowJs": true,
    "declaration": false,
    "outDir": "dist/",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "src/",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/**/*"]
}
